{
    "manifest_version": 3,
    "name": "YouTube ad skip",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "webNavigation"
    ],
    "action": {
      "default_icon": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["content.js"]
        }
      ]
  }